{
  "input": {
    "input_file_name": "tutorial.root",
    "input_name": ["histMass"],
    "pdf_dictionary": {
      "pdf": ["Gaus", "Gaus", "Exp", "SUM"],
      "pdfName": ["Jpsi", "Psi2s", "Bkg", "SUM"],
      "pdfNameForLegend": ["J/#psi", "#psi(2S)", "Background", "Fit"],
      "pdfColor": [861, 417, 1],
      "pdfStyle": [1, 1, 2],
      "parVal": [
        [3.096, 0.6e-01],
        [3.096, 0.6e-01],
        [1, 1],
        [1000, 100, 100000]
      ],
      "parLimMin": [
        [2.9, 7.0e-02],
        [2.9, 7.0e-02],
        [-10, 0],
        [1, 1, 1]
      ],
      "parLimMax": [
        [3.5, 1.9e-01],
        [3.9, 1.9e-01],
        [10, 100],
        [5000000, 5000000, 50000000]
      ],
      "parName": [
        ["mean_Jpsi", "width_Jpsi"],
        ["sum::mean_Psi2s(mean_Jpsi,0.584)", "prod::width_Psi2s(width_Jpsi,1.05)"],
        ["aa", "bb"],
        ["sig_Jpsi", "sig_Psi2s", "bkg"]
      ],
      "fitRangeMin": [2.2, 2],
      "fitRangeMax": [4.8, 5],
      "doResidualPlot": true,
      "doPullPlot": true, 
      "doCorrMatPlot": true,
      "doAlicePlot": true,
      "parForAlicePlot": ["mean_Jpsi", "width_Jpsi", "sig_Jpsi", "sig_Psi2s"],
      "parNameForAlicePlot": ["#mu_{J/#psi}", "#sigma_{J/#psi}", "#it{N}_{J/#psi}","#it{N}_{#psi(2S)}"],
      "text": [
        [0.20, 0.87, "Simulation, toy MC"],
        [0.20, 0.80, "2.5 < #it{y} < 4, J/#psi #rightarrow #mu^{+}#mu^{-}"]
      ]
    }
  },
  "output": {
    "output_file_name": "output/"
  }
}